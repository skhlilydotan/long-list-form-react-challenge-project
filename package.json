    "@mui/material": "^5.14.4",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "classnames": "^2.3.2",
    "dompurify": "^3.0.6",
    "highcharts-react-official": "^3.2.1",
    "jest": "^29.7.0",
